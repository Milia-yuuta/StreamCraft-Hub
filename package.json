{
  "name": "streamcraft-hub",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "lint": "eslint --fix src && prettier --write . --plugin=prettier-plugin-astro",
    "dev": "eslint --fix src && prettier --write . --plugin=prettier-plugin-astro && astro dev",
    "start": "eslint --fix src && prettier --write . --plugin=prettier-plugin-astro && astro check && astro dev",
    "build": "eslint --fix src && prettier --write . --plugin=prettier-plugin-astro && astro check && astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.6",
    "@astrojs/mdx": "^2.1.1",
    "@astrojs/rss": "^4.0.5",
    "@astrojs/sitemap": "^3.1.1",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/vercel": "^7.3.4",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "astro": "^4.4.6",
    "eslint": "^8.57.0",
    "pnpm": "^8.15.4",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "tailwind": "^4.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  }
}
